{
  "manifest_version": 3,
  "name": "Canvas Syllable Extractor",
  "version": "0.2.0",
  "description": "Hookt Canvas fillText, sammelt Silben mit Anzahl und kopiert sie.",
  "permissions": ["clipboardWrite"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["pageHook.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
